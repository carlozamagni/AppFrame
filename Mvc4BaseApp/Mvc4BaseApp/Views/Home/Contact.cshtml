@{
    ViewBag.Title = "Contact";
}


<div class="container-fluid">
    <div class="row-fluid">

        <div class="span3">
            <p>
                <a href="#" class="thumbnail">
                    <img src="~/Content/images/profile-icon.png" alt="contact photo" />
                </a>
            </p>


        </div>
        <!--/span-->
        <div class="span9">
            @*<span class="label label-info">support</span><span><a href="mailto:Support@example.com">Support@example.com</a></span><br />
            <br />
            <span class="label label-info">marketing</span><span><a href="mailto:Marketing@example.com">Marketing@example.com</a></span><br />
            <br />
            <span class="label label-info">general</span><span><a href="mailto:General@example.com">General@example.com</a></span><br />
            <br />*@

            <table class="table table-hover">
                <caption>Contacts</caption>
                <thead>
                    <tr>
                        <th>Department</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><span class="">support</span></td>
                        <td><a href="mailto:Support@example.com">Support@example.com</a></td>
                    </tr>
                    <tr>
                        <td><span class="">marketing</span></td>
                        <td><a href="mailto:Marketing@example.com">Marketing@example.com</a></td>
                    </tr>
                    <tr class="info">
                        <td><span class="label label-info">info:</span><span class="">&nbsp;general</span></td>
                        <td><a href="mailto:General@example.com">General@example.com</a></td>
                    </tr>
                </tbody>
            </table>


        </div>
    </div>

    <div class="row-fluid" style="text-align: center">
        <p>
            <a href="https://github.com/carlozamagni/Mvc4AppFoundation" class="btn btn-primary btn-large" type="button">&raquo;look at the code on github</a>
        </p>
    </div>
</div>


