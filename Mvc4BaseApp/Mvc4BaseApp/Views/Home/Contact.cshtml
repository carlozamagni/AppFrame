@{
    ViewBag.Title = "Contact";
}


<div class="container-fluid">
    <div class="row-fluid">

        <div class="span3">
            <a href="#" class="thumbnail">
                <img src="@ViewBag.PhotoUrl" alt="contact photo" />
            </a>

        </div>
        <!--/span-->
        <div class="span9">
            <span class="label label-info">support</span><span><a href="mailto:Support@example.com">Support@example.com</a></span>
            <span class="label label-info">marketing</span><span><a href="mailto:Marketing@example.com">Marketing@example.com</a></span>
            <span class="label label-info">general</span><span><a href="mailto:General@example.com">General@example.com</a></span>
            <span class="label">url</span><span><a href="https://github.com/carlozamagni/Mvc4AppFoundation">look at the code on github</a></span>
        </div>
    </div>
</div>


